	<h1>숫자와 문자열의 덧셈</h1>
	<p id="result1"></p>
  <p id="result2"></p>

	<script>
		var num = 10;
		var str = "JavaScript";
		
		document.getElementById("result1").innerHTML = (num + str); // result인 ID를 찾아서 내부를 (num + str)로 변경해라

    var firstNum = 10;
		var secondNum = 11;
		
		document.getElementById("result2").innerHTML = (firstNum == secondNum);
	</script>
	
